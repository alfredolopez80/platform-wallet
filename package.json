  "scripts": {
    "start": "export BROWSER=none PORT=3100 REACT_APP_ENV=development && react-scripts-ts start",
    "start:staging": "export BROWSER=none PORT=3100 REACT_APP_ENV=staging && react-scripts-ts start",
    "start:production": "export BROWSER=none PORT=3100 REACT_APP_ENV=production && react-scripts-ts start",
    "build": "export REACT_APP_ENV=production && react-scripts-ts --max_old_space_size=2048 build",
    "build:staging": "export REACT_APP_ENV=staging && react-scripts-ts --max_old_space_size=2048 build",
    "test": "react-scripts-ts test --env=jsdom",
    "lint": "tslint --project tsconfig.json",
    "lint:fix": "tslint --fix --project tsconfig.json",
    "electron": "electron .",
    "pack": "tsc && electron-builder --dir",
    "dist": "tsc && electron-builder",
    "release": "tsc && electron-builder --mac --win --linux",
    "dist:windows": "tsc && electron-builder --win",
    "dist:linux": "tsc && electron-builder --linux",
    "dist:mac": "tsc && electron-builder --mac",
    "dist:all": "tsc && electron-builder --mac --win --linux",
    "postinstall": "./node_modules/.bin/electron-builder install-app-deps"
  },
