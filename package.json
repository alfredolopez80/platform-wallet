{
  "name": "dbet-wallet",
  "description": "DBET Wallet",
  "version": "0.1.0",
  "author": "Decent.bet",
  "private": true,
  "devDependencies": {
    "autoprefixer": "6.5.1",
    "babel-core": "6.17.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "17.0.2",
    "babel-loader": "6.2.7",
    "babel-preset-react-app": "^2.0.1",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.2",
    "css-loader": "0.26.0",
    "detect-port": "1.0.1",
    "dotenv": "2.0.0",
    "electron": "^1.7.9",
    "electron-builder": "^19.40.0",
    "electron-packager": "^9.1.0",
    "eslint": "3.8.1",
    "eslint-config-react-app": "^0.5.0",
    "eslint-loader": "1.6.0",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "filesize": "3.3.0",
    "fs-extra": "0.30.0",
    "gzip-size": "3.0.0",
    "html-webpack-plugin": "2.24.0",
    "http-proxy-middleware": "0.17.2",
    "jest": "18.1.0",
    "json-loader": "0.5.4",
    "object-assign": "4.1.0",
    "path-exists": "2.1.0",
    "postcss-loader": "1.0.0",
    "promise": "7.1.1",
    "react-dev-utils": "^0.4.2",
    "recursive-readdir": "2.1.0",
    "strip-ansi": "3.0.1",
    "style-loader": "0.13.1",
    "truffle-contract": "^1.1.11",
    "truffle-solidity-loader": "0.0.8",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "1.0.0"
  },
  "dependencies": {
    "bip39": "^2.4.0",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^2.0.0",
    "electron-simple-updater": "^1.2.1",
    "electron-updater": "^2.15.0",
    "ethereum-address": "0.0.4",
    "ethereum-units": "0.0.1-b",
    "ethers": "^2.1.2",
    "ethjs-provider-signer": "^0.1.4",
    "ethjs-signer": "^0.1.1",
    "eventing-bus": "^1.3.3",
    "express": "^4.16.2",
    "material-ui": "^0.19.1",
    "react": "^15.6.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^15.4.2",
    "react-fittext": "^1.0.0",
    "react-material-ui-keyboard": "^6.2.1",
    "react-materialui-notifications": "^0.5.1",
    "react-router": "^3.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "request": "^2.83.0",
    "web3-eth-abi": "^1.0.0-beta.24",
    "web3-eth-accounts": "^1.0.0-beta.24"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "electron": "electron .",
    "pack": "build --dir",
    "dist": "build --win --x64 --ia32",
    "postinstall": "install-app-deps",
    "package-win": "electron-packager . --platform=win32 --arch=ia32 --ignore=.idea --ignore=contracts --ignore=config --ignore=migrations --ignore=node_modules --ignore=public --ignore=scripts --ignore=test --ignore=truffle-config.js --asar --prune --icon=public/assets/icons/favicon-32x32.png --overwrite --out=release-builds",
    "package-osx": "electron-packager . --platform=darwin --arch=x64 --ignore=.idea --ignore=contracts --ignore=migrations --icon=public/assets/icons/favicon-32x32.icns --asar --overwrite --out=release-builds",
    "package-linux": "electron-packager . --platform=linux arch=x64 --ignore=.idea --ignore=contracts --ignore=migrations --icon=public/assets/icons/favicon-32x32.ico --asar --overwrite --out=release-builds"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ]
  },
  "main": "src/electron.js",
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "build": {
    "appId": "wallet.decent.bet",
    "productName": "DBET Wallet",
    "files": [
      "**/*",
      "!dist{/*}",
      "!release-builds",
      "!.idea{/*}",
      "!contracts{/*}",
      "!config{/*}",
      "!migrations{/*}",
      "!node_modules{/*}",
      "!public{/*}",
      "!scripts{/*}",
      "!test{/*}",
      "!truffle-config.js"
    ],
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "rpm",
        "tar.gz"
      ]
    },
    "win": {
      "icon": "./public/assets/icons/icon-256"
    },
    "directories": {
      "buildResources": "public"
    }
  }
}
